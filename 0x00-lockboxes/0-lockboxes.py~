#!/usr/bin/python3
""" Module that checks locked boxes """


def canUnlockAll(boxes):
    """ Method that determines if all boxes can be opened """
    if boxes == []:
        return True

    l = len(boxes)

    for x in range(l):
        print("first", boxes[x])

    for i in boxes:
        print("second", i)

    list_ing = [i for i in range(l)]
    lst = [x for l in boxes for x in l]

    print(list_ing)
    print(lst)
    print(boxes)


    for x in boxes:
        for y in x:
            print(y)
